#include <iostream>

#include <conio.h>
 
const int n = 10;

const int m = 10;
 
int main () {
setlocale(LC_ALL, "Russian");
 
int X[n][m]; 
int i,j;    
 
for (i = 0; i < n ; i++)

for (j = 0; j < m ; j++){   
std::cout<<"Введите значение Х["<<i<<","<<j<<"] = "; 
std::cin>>X[i][j];  
}
std::cout<<"\n"; 
std::cout<<"Ваш массив"<<"\n";
for (i = 0; i < n ; i++){   
std::cout<<"\n";
for (j = 0; j < n ; j++){
    
    std::cout<<X[i][j]<<"\t"; 
}
}
getch();    
}
